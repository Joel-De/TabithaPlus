{
  "name": "Memory Manager",
  "description": "A browser extension that can manage google chrome tabs to optimize memory footprint",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "tabs",
    "storage"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_title": "Memory Manager",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "key": ""
}
